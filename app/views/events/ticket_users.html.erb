<% @users.each do |user| %>
  <ul class="ticket_users">
    <li><%= link_to image_tag(user.image.variant(combine_options:{gravity: :center, resize:"100x100^", crop:"100x100+0+0"}).processed, class: "ticket-image"), user_path(user) %></li>
    <li class="ticket_users_name"><%= user.name %></li>
  </ul>
  <hr>
<% end %>
